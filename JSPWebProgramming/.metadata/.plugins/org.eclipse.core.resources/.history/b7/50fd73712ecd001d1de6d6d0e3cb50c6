package ch08;

public class Product {
	private String id;
	private String name;
	private String maker;
	private int price;
	private String date;
	
	//데이터 생성을 쉽게하기위한 생성
	public Product(String id, String name, String maker, int price, String date) {
		super();
		this.id = id;
		this.name = name;
		this.maker = maker;
		this.price = price;
		this.date = date;
	}
	
	//getter,setter 메소
	
	

}
